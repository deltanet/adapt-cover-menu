<div class="menu-item-inner" role="menuitem">
	<div class="menu-item-title" role="heading">
		<div class="menu-item-title-inner">{{{a11y_text title}}}</div>
	</div>
	<div class="menu-item-body">
		<div class="menu-item-body-inner">{{{a11y_text body}}}</div>
	</div>
	<div class="menu-item-duration">
		<div class="menu-item-duration-inner">{{{a11y_text _coverMenuAudio.durationLabel}}} {{{a11y_text duration}}}</div>
	</div>
	<button aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="menu-item-route {{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block">
    {{#if _isLocked}}
    {{{_coverMenuAudio.linkLockedText}}}
        {{else}}
		{{#if _isVisited}}
		    {{{_coverMenuAudio.linkVisitedText}}}
		    {{else}}
		    {{{linkText}}}
		{{/if}}
	{{/if}}
    </button>
	<div class="menu-item-spacer"></div>
</div>
