<div class="menu-item-indicator-inner" role="menuitem">
	<a href="#" class="menu-item-indicator-graphic" aria-label="{{_coverMenu_ariaLabels.menuItemPage}}">
		{{#if _isComplete}}
			{{#if _assessment}}
				{{#if _assessment.isPassed}}
					<img src="{{_coverMenuAudio._indicatorGraphic._isPassed}}"/>
				{{else}}
					<img src="{{_coverMenuAudio._indicatorGraphic._isFailed}}"/>
				{{/if}}
			{{else}}
				<img src="{{_coverMenuAudio._indicatorGraphic._isComplete}}"/>
			{{/if}}
		{{else}}
			{{#if _isVisited}}
				<img src="{{_coverMenuAudio._indicatorGraphic._isVisited}}"/>
			{{else}}
				{{#if _isLocked}}
					<img src="{{_coverMenuAudio._indicatorGraphic._isLocked}}"/>
				{{else}}
					<img class="accessibility-indicator-graphic" src="{{_coverMenuAudio._indicatorGraphic._default}}"/>
				{{/if}}
			{{/if}}
		{{/if}}
	</a>
</div>
